{
  "replacementPatterns": [
    {
      "pattern": "^@site/",
      "replacement": "{{BASEURL}}/"
    },
    {
      "pattern": "^/img/",
      "replacement": "{{BASEURL}}/static/img/"
    }
  ],
  "aliveStatusCodes": [
    200,
    403
  ],
  "ignorePatterns": [
    {
      "pattern": "^#.+"
    },
    {
      "pattern": "/(localhost)."
    },
    {
      "pattern": "^/develop/"
    },
    {
      "pattern": "^/participate/"
    },
    {
      "pattern": "^/concepts/"
    }
  ]
}